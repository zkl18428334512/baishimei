<view class="page"><view class="cu-bar bg-white solid-bottom padding"><block wx:if="{{hasLogin}}"><view style="font-size:50rpx;color:#007BFF;">{{userInfo.name}}</view></block></view><view class="cu-bar bg-white solid-bottom " style="margin-top:-40rpx;"><view class="action">{{'职能：'+Obtainposition1+''}}</view></view><view class="{{['cu-list grid','col-'+gridCol,gridBorder?'':'no-border']}}"><block wx:for="{{cuIconList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{!item.hidden}}"><view class="cu-item" style="{{'width:350rpx;margin-left:10rpx;margin-top:10rpx;height:440rpx;'+('background-image:'+('url('+item.url+')')+';')+('background-size:'+('cover')+';')}}"><view data-event-opts="{{[['tap',[['handleTap',['$0'],[[['cuIconList','',index,'path']]]]]]]}}" class="{{['cuIcon-'+item.cuIcon,'text-'+item.color]}}" style="margin-top:100rpx;" bindtap="__e"></view><text style="color:#F8F9FA;">{{item.name}}</text><block wx:if="{{item.badge!=0}}"><view><text class="cuIcon-title text-white " style="color:#F8F9FA;font-size:20rpx;"><block wx:if="{{item.badge!=1}}"><block>{{item.badge>99?'99+':item.badge}}</block></block>个进行中</text></view></block></view></block></block></view><block wx:if="{{loadModal}}"><view class="cu-load load-modal"><image src="/static/logo.png" mode="aspectFit"></image><view class="gray-text">加载中...</view></view></block></view>